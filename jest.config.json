{
  "preset": "ts-jest",
  "testEnvironment": "jsdom",
  "setupFilesAfterEnv": ["<rootDir>/setupTests.ts"],
  "testPathIgnorePatterns": ["/node_modules/", "/.next/"],
  "transform": {
    "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
  },
  "transformIgnorePatterns": ["/node_modules/"]
}
